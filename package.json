{
  "name": "saw-universal",
  "version": "0.0.0",
  "engines": {
    "node": "12.18.1"
  },
  "scripts": {
    "ng": "ng",
    "start": "node dist/server/main.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run saw-universal:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --configuration=production && ng run saw-universal:server:production && cp package.json dist && cp -r .ebextensions dist && cp -r .platform dist",
    "build:ssr-dev": "ng build --configuration=development && ng run saw-universal:server:development",
    "prerender": "ng run saw-universal:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.2",
    "@angular/common": "~10.1.2",
    "@angular/compiler": "~10.1.2",
    "@angular/core": "~10.1.2",
    "@angular/forms": "~10.1.2",
    "@angular/platform-browser": "~10.1.2",
    "@angular/platform-browser-dynamic": "~10.1.2",
    "@angular/platform-server": "~10.1.2",
    "@angular/router": "~10.1.2",
    "@ng-bootstrap/ng-bootstrap": "^8.0.0",
    "@nguniversal/express-engine": "^10.1.0",
    "@ngx-lite/json-ld": "^0.6.2",
    "axios": "^0.21.0",
    "bootstrap": "^4.5.2",
    "domino": "^2.1.6",
    "express": "^4.15.2",
    "jquery": "^3.4.1",
    "ng-recaptcha": "^6.0.2",
    "ng2-page-scroll": "^4.0.0-beta.12",
    "ngx-page-scroll": "^7.0.3",
